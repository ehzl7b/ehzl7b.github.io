<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {%- for x in pagesMap -%}
    {% if x[0] == "_dir" or x[0] == "" %} {% continue %} {% endif %}
    {%- for page in x[1] -%}
        <url><loc>{{ page.permalink }}</loc><lastmod>{{ page.updated }}</lastmod></url>
    {%- endfor -%}
  {%- endfor %}
</urlset>